          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript Async="1">{$lua}

function SlotsUnrandomizer()
  randomPtr = readBytes('randomPtr', 1, false)
  if randomPtr == nil or randomPtr == 1 or randomPtr == true then beep(); return end

  playSound(findTableFile('Activate'))

  -- Enable (set randomPtr to 1)
  writeBytes("randomPtr", 1)
  
  -- wait a bit
  sleep(100)

  -- Press E key
  doKeyPress(VK_E)

  -- wait a bit
  sleep(200)

  -- Disable (set randomPtr to 0)
  writeBytes("randomPtr", 0)

  playSound(findTableFile('Deactivate'))

end

hkey = createHotkey("SlotsUnrandomizer", VK_ADD)
hkey.OnHotkey = SlotsUnrandomizer
hkey.setKeys(VK_ADD)

{$asm}

[ENABLE]
aobscanmodule(random,ucrtbase.dll,00 8B C1 48 83 C4 28)
alloc(newmem,$1000,random)

label(code)
label(return)
label(randomPtr)

newmem:
  mov rax,randomPtr
  cmp byte ptr [rax],1
  jne code
  mov ecx,dword ptr [rax+1]
code:
  mov eax,ecx
  add rsp,28
  jmp return

randomPtr:
  dq 0

random+01:
  jmp newmem
  nop
return:
registersymbol(random)
registersymbol(randomPtr)

[DISABLE]
random+01:
  db 8B C1 48 83 C4 28
unregistersymbol(random)
unregistersymbol(randomPtr)
dealloc(newmem)
</AssemblerScript>

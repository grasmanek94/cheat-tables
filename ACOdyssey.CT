newmem_inv:
  extract_params:
      //---
      mov rax, scan_params2_inv
      mov ebx, [rax+3]
      movsxd rbx, ebx
      mov rax, [rax+rbx+7]
      mov [PlayerProgressionManager], rax
      //---
      mov rax, scan_params2_inv+0D
      mov ebx, [rax+1]
      movsxd rbx, ebx
      lea  rax, [rax+rbx+5]
      mov [sub_GetEntity], rax
      //---
